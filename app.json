{
  "expo": {
    "name": "HRM",
    "sdkVersion": "52.0.0",
    "slug": "Hotel Room Monitor",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "../Images/HRM.png",
    "scheme": "myapp",
    "userInterfaceStyle": "automatic",
    "newArchEnabled": true,
    "jsEngine": "hermes",
    "ios": {
      "supportsTablet": true,
      "permissions": [
        "CAMERA"
      ]
    },
    "android": {
      "permissions": [
        "CAMERA"
      ],
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "package": "com.anonymous.HotelRoomMonitor"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      [
        "expo-splash-screen",
        {
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 200,
          "resizeMode": "contain",
          "backgroundColor": "#ffffff"
        }
      ],
      "expo-router",
      "expo-sqlite"
    ],
    "experiments": {
      "typedRoutes": true,
      "bridgeless": false
    },
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "1bb92933-45c4-4d31-8f6c-78c192fa56c7"
      }
    }
  }
}
